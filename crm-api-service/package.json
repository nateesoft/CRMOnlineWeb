{
  "name": "crm-api-service",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:pm2": "pm2 start ./bin/www --name crm-api-service -i max",
    "start:demo": "cross-env API_PORT=8080 API_SERVER=demo node ./bin/www",
    "start:demo:pm2": "cross-env API_PORT=8080 API_SERVER=demo pm2 start ./bin/www --name crm-api-service",
    "watch": "nodemon ./bin/www",
    "watch:demo": "cross-env API_PORT=8080 API_SERVER=demo nodemon ./bin/www",
    "test": "mocha ./routes/tests --exit ",
    "test-with-coverage": "nyc --reporter=html mocha ./routes/tests",
    "generate": "plop --plopfile internals/generators/index.js"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "coupon-code": "^0.4.5",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.2",
    "express": "~4.16.1",
    "express-basic-auth": "^1.2.0",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.0",
    "http-errors": "~1.6.3",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "nocache": "^2.1.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.3",
    "plop": "^2.7.4",
    "qrcode": "^1.4.4",
    "qrcode-reader": "^1.0.4",
    "request": "^2.88.2",
    "socket.io": "^2.4.0",
    "swagger-jsdoc": "^4.2.3",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0"
  }
}
